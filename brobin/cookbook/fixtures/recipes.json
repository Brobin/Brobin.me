[{"model": "cookbook.recipe", "pk": 1, "fields": {"title": "Nashville Hot Chicken", "created": "2020-07-22T20:15:44.599Z", "updated": "2020-07-22T20:15:54.648Z", "slug": "nashville-hot-chicken", "notes": "A spicy fried chicken recipe that's super simple to make and very satisfying! I usually use chicken thighs. They usually run cheaper and the dark meat complements the spice very well.", "prep_time": null, "cook_time": null}}, {"model": "cookbook.recipe", "pk": 2, "fields": {"title": "Soft Pretzels", "created": "2020-07-22T20:37:24.303Z", "updated": "2020-07-22T20:37:55.236Z", "slug": "soft-pretzels", "notes": "Top these with the classic salt, or cheese and japaneos for more of a kick. Make sure to roll these out more than you think, when they rise in the oven they may end up more like bagels if you don't roll them thin enough.", "prep_time": null, "cook_time": null}}, {"model": "cookbook.ingredient", "pk": 1, "fields": {"name": "Chicken"}}, {"model": "cookbook.ingredient", "pk": 2, "fields": {"name": "Salt"}}, {"model": "cookbook.ingredient", "pk": 3, "fields": {"name": "Black Pepper"}}, {"model": "cookbook.ingredient", "pk": 4, "fields": {"name": "Buttermilk"}}, {"model": "cookbook.ingredient", "pk": 5, "fields": {"name": "Louisiana Style Hot Sauce"}}, {"model": "cookbook.ingredient", "pk": 6, "fields": {"name": "Flour (all-purpose)"}}, {"model": "cookbook.ingredient", "pk": 7, "fields": {"name": "Cornstarch"}}, {"model": "cookbook.ingredient", "pk": 8, "fields": {"name": "Seasoned Salt"}}, {"model": "cookbook.ingredient", "pk": 9, "fields": {"name": "Cayenne Pepper"}}, {"model": "cookbook.ingredient", "pk": 10, "fields": {"name": "Onion Powder"}}, {"model": "cookbook.ingredient", "pk": 11, "fields": {"name": "Garlic Powder"}}, {"model": "cookbook.ingredient", "pk": 12, "fields": {"name": "Brown Sugar"}}, {"model": "cookbook.ingredient", "pk": 13, "fields": {"name": "Paprika"}}, {"model": "cookbook.ingredient", "pk": 14, "fields": {"name": "Chili Powder"}}, {"mode